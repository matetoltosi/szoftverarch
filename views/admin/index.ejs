<%- include('../partials/header', { title: 'Admin Panel' }) %>

<div class="card">
  <div class="card-header">
    <h1 class="card-title">Admin Panel</h1>
  </div>

  <p><a href="/">Back to home</a></p>
  <h2>Users</h2>
  <ul>
    <% users.forEach(u => { %>
      <li><%= u.username %> — <%= u.role %> — <%= u.isActive ? 'active' : 'inactive' %></li>
    <% }) %>
  </ul>
</div>

<%- include('../partials/footer') %>